# Capture encoder position as it changes
*VERBOSE=1
*PCAP.DISARM=
*CAPTURE=
INENC1.PROTOCOL=BISS
OUTENC1.PROTOCOL=BISS
INENC1.BITS_CRC=8
INENC1.BITS=19
PCOMP1.ENABLE=PCAP.ACTIVE
PCOMP1.INP=INENC1.VAL
PCOMP1.DELTAP=100
PCOMP1.START=1000
PCOMP1.STEP=0
PCOMP1.WIDTH=9000
PCOMP1.DIR=Positive
PCOMP1.PNUM=1
PCOMP1.RELATIVE=Absolute
PCOMP1.USE_TABLE=No
PCAP.ENABLE=PCOMP1.ACTIVE
PCAP.ENABLE.DELAY=0
PCAP.FRAME=BITS.ZERO
PCAP.CAPTURE=INENC1.TRANS
INENC1.VAL.CAPTURE=Triggered
*PCAP.ARM=
