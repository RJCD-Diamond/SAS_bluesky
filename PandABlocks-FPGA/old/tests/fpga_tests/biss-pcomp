# Single PComp using BISS encoder
*VERBOSE=1
*PCAP.DISARM=
*CAPTURE=
INENC1.PROTOCOL=BISS
OUTENC1.PROTOCOL=BISS
INENC1.BITS_CRC=6
INENC1.BITS=19
PCOMP1.ENABLE=PCAP.ACTIVE
PCOMP1.INP=INENC1.VAL
PCOMP1.DELTAP=10
PCOMP1.START=100
PCOMP1.STEP=100
PCOMP1.WIDTH=10
PCOMP1.DIR=Positive
PCOMP1.PNUM=5
PCOMP1.RELATIVE=Absolute
PCOMP1.USE_TABLE=No
PCAP.ENABLE=PCOMP1.ACTIVE
PCAP.ENABLE.DELAY=0
PCAP.FRAME=BITS.ZERO
PCAP.CAPTURE=PCOMP1.OUT
INENC1.VAL.CAPTURE=Triggered
PCAP.CAPTURE_TS.CAPTURE=Trigger
*PCAP.ARM=
